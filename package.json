{
  "name": "mat-datetimepicker",
  "version": "5.1.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "yarn build:lib && ng build",
    "build:prod": "yarn build:lib && ng build --prod --stats-json",
    "build:lib": "yarn build:lib/core && yarn build:lib/moment",
    "build:lib/core": "ng build core --prod && copy-files-from-to --config projects/core/copy-files.json",
    "build:lib/moment": "ng build moment --prod && copy-files-from-to --config projects/moment/copy-files.json",
    "bundle-report": "webpack-bundle-analyzer dist/app/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "~10.2.3",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/material": "~10.2.3",
    "@angular/material-moment-adapter": "~10.2.3",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "copy-files-from-to": "3.1.4",
    "moment": "2.27.0",
    "rxjs": "~6.6.2",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "^11.1.4",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.0",
    "protractor": "~7.0.0",
    "rollup": "^2.23.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "4.1.5",
    "webpack-bundle-analyzer": "3.8.0"
  }
}